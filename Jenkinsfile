pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World from Jenkins'
            }
        }
        stage('Webhook') {
            steps {
                echo 'Testing the webhook with GitHub'
            }
        }
        stage('Build') {
            steps {
                echo 'Running the Jenkins Pipeline'
            }
        }
    }
}
